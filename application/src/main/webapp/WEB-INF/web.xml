<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>

  <display-name>Archetype Created Web Application</display-name>
  <servlet>
  	<servlet-name>register</servlet-name>
  	<display-name>register</display-name>
  	<description></description>
  	<servlet-class>application.register</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>login</servlet-name>
  	<display-name>login</display-name>
  	<description></description>
  	<servlet-class>application.login</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>viewTimeTable</servlet-name>
  	<display-name>viewTimeTable</display-name>
  	<description></description>
  	<servlet-class>application.viewTimeTable</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>ClassController</servlet-name>
  	<display-name>ClassController</display-name>
  	<description></description>
  	<servlet-class>application.ClassController</servlet-class>
  </servlet>
  
  <servlet-mapping>
  	<servlet-name>register</servlet-name>
  	<url-pattern>/register</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>login</servlet-name>
  	<url-pattern>/login</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>viewTimeTable</servlet-name>
  	<url-pattern>/viewTimeTable</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>ClassController</servlet-name>
  	<url-pattern>/ClassController</url-pattern>
  </servlet-mapping>

</web-app>
