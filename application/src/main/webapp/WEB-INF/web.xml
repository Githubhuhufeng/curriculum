<?xml version="1.0" encoding="UTF-8"?>

<web-app 
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
    xmlns="http://java.sun.com/xml/ns/javaee"
    xmlns:web="http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" 
    xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
    id="Your_WebApp_ID"
    version="2.5">

  <display-name>Archetype Created Web Application</display-name>
  <servlet>
  	<display-name>register</display-name>
  	<servlet-name>register</servlet-name>
  	<servlet-class>application.Register</servlet-class>
  </servlet>
  <servlet>
  	<display-name>login</display-name>
  	<servlet-name>login</servlet-name>
  	<servlet-class>application.Login</servlet-class>
  </servlet>
  <servlet>
  	<display-name>viewTimeTable</display-name>
  	<servlet-name>viewTimeTable</servlet-name>
  	<servlet-class>application.ViewTimeTable</servlet-class>
  </servlet>
  <servlet>
  	<display-name>classController</display-name>
  	<servlet-name>classController</servlet-name>
  	<servlet-class>application.ClassController</servlet-class>
  </servlet>
  <servlet>
  	<display-name>teachers</display-name>
  	<servlet-name>teachers</servlet-name>
  	<servlet-class>application.Teachers</servlet-class>
  </servlet>
  <servlet>
  	<display-name>classList</display-name>
  	<servlet-name>classList</servlet-name>
  	<servlet-class>application.ClassList</servlet-class>
  </servlet>
  <servlet>
    <display-name>classForm</display-name>
    <servlet-name>classForm</servlet-name>
    <servlet-class>application.ClassForm</servlet-class>
  </servlet>
  <servlet>
    <display-name>editClassTime</display-name>
    <servlet-name>editClassTime</servlet-name>
    <servlet-class>application.EditClassTime</servlet-class>
  </servlet>
  <servlet>
  	<display-name>updateTime</display-name>
  	<servlet-name>updateTime</servlet-name>
  	<servlet-class>application.UpdateTime</servlet-class>
  </servlet>
  <servlet>
    <display-name>editTimeTable</display-name>
    <servlet-name>editTimeTable</servlet-name>
    <servlet-class>application.EditTimeTable</servlet-class>
  </servlet>
  
  
  <servlet-mapping>
  	<servlet-name>register</servlet-name>
  	<url-pattern>/register</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>login</servlet-name>
  	<url-pattern>/login</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>viewTimeTable</servlet-name>
  	<url-pattern>/viewTimeTable</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>classController</servlet-name>
  	<url-pattern>/classController</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>teachers</servlet-name>
  	<url-pattern>/teachers</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>classList</servlet-name>
  	<url-pattern>/classList</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>classForm</servlet-name>
    <url-pattern>/classForm</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>editClassTime</servlet-name>
    <url-pattern>/editClassTime</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>updateTime</servlet-name>
    <url-pattern>/updateTime</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>editTimeTable</servlet-name>
    <url-pattern>/editTimeTable</url-pattern>
  </servlet-mapping>


</web-app>
